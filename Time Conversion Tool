using System;

class Program
{
    static void Main()
    {
        int[] durations = new int[10];
        Console.WriteLine("Enter 10 durations in seconds:");

        for (int i = 0; i < 10; i++)
            durations[i] = int.Parse(Console.ReadLine());

        Console.WriteLine("\nSeconds\t\tHH:MM:SS");
        foreach (int sec in durations)
        {
            TimeSpan t = TimeSpan.FromSeconds(sec);
            Console.WriteLine($"{sec}\t\t{t:hh\\:mm\\:ss}");
        }
    }
}
