using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        Console.WriteLine("Enter at least 10 integers (space-separated):");
        var nums = Array.ConvertAll(Console.ReadLine().Split(), int.Parse);

        if (nums.Length < 10)
        {
            Console.WriteLine("List must contain at least 10 integers.");
            return;
        }

        Console.Write("Enter window size k: ");
        int k = int.Parse(Console.ReadLine());

        if (k > nums.Length || k < 1)
        {
            Console.WriteLine("Invalid window size.");
            return;
        }

        Console.WriteLine($"\nSliding window sums (k={k}):");
        for (int i = 0; i <= nums.Length - k; i++)
        {
            int sum = 0;
            for (int j = 0; j < k; j++)
                sum += nums[i + j];
            Console.WriteLine($"Window {i + 1}: Sum = {sum}");
        }
    }
}
